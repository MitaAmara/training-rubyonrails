<%= form_for([@article, @article.comments.build], remote:true, html: {id: :form_id}) do |f| %>
  <%= f.label :commenter %>
  <%= f.text_field :commenter %>
  <br />
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit :Submit, :class => 'btn btn-outline btn-outline-info' %> 
<% end %>